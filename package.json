  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/rsocket-websocket-client": "^0.0.7",
    "@types/ws": "^8.18.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@powersync/service-core": "^1.14.0",
    "commander": "^14.0.0",
    "jose": "^6.0.13",
    "rsocket-core": "1.0.0-alpha.3",
    "rsocket-websocket-client": "1.0.0-alpha.3",
    "ws": "^8.18.3",
    "yaml": "^2.8.1"
  }
}
